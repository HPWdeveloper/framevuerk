{
  "name": "framevuerk",
  "version": "0.99.4",
  "description": "Awesome fully responsive Vue.js framevuerk!",
  "main": "dist/framevuerk-en.js",
  "scripts": {
    "lint:js": "eslint ./ --ext .js,.vue",
    "lint:js:fix": "eslint ./ --fix --ext .js,.vue",
    "lint:scss": "stylelint '**/*.{vue,scss}' --syntax scss",
    "lint:scss:fix": "echo \"Error: sorry. We have no scss lint fixer now. Use local editor extension instead.\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "test": "npm run lint",
    "clean:lib": "rimraf ./dist",
    "build:lib": "npm run clean:lib && cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --hide-modules --progress",
    "clean:docs": "rimraf ./docs",
    "build:docs": "npm run clean:docs && cross-env NODE_ENV=production webpack --config ./build/webpack.config.docs.js",
    "clean": "npm run clean:lib && npm run clean:docs",
    "build": "npm run build:lib && npm run build:docs",
    "publish-all": "./build/publish-all.sh",
    "dev": "webpack --config ./build/webpack.config.js --display none -w & webpack-dev-server --inline --config ./build/webpack.config.docs.js --host 0 --open --disable-host-check",
    "start": "npm run build && http-server ./docs"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "axios": "^0.16.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-minify-simplify": "^0.2.0",
    "babel-plugin-transform-minify-booleans": "^6.8.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-minify": "^0.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.26.2",
    "cssnano": "^3.10.0",
    "cssnano-preset-default": "^4.0.0-rc.2",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-unicorn": "^2.1.2",
    "eslint-plugin-vue": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "lite-server": "^2.2.2",
    "moment": "^2.18.1",
    "moment-jalaali": "^0.7.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "pug-html-loader": "^1.1.5",
    "pug-lint": "^2.4.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^2.2.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.8",
    "vue": "^2.3.4",
    "vue-highlightjs": "^1.3.3",
    "vue-loader": "^13.7.0",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.3.4",
    "vue-template-compiler-loader": "^1.0.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framevuerk/framevuerk.git"
  },
  "keywords": [
    "vue",
    "framework",
    "framevuerk",
    "rtl",
    "ltr"
  ],
